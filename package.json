{
  "name": "react-duo-web",
  "version": "0.0.1",
  "description": "React component wrapper for the Duo Web SDK",
  "main": "index.js",
  "scripts": {
    "build": "mkdir -p dist && webpack --config config/webpack.js",
    "lint": "eslint src lib test",
    "test": "node test | tap-spec",
